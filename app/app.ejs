<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>TJMC-Launcher</title>
    <meta http-equiv="Content-Security-Policy" content="script-src 'self'">
    <!--<script src="./assets/js/uicore.js"></script>-->
    <link type="text/css" rel="stylesheet" href="./assets/css/launcher.css">
</head>
<body class="preload">
    <%- include('frame') %>
    <div id="preloader">
        <!--<div class="big-text">TJMC-Launcher</div>-->
        <img src="./assets/images/icon.png">
    </div>
    <div id="app-mount">
        <div id="frameMain" class="app-layer frame-fix">
            <div id="topBar" class="hidden">
                <div id="progress-bar"></div>
            </div>
            <div id="main">
                <%- include('main') %>
            </div>
            <div id="mainBar">
                <input type="text" placeholder="Ник" id="nick">
                <select id="version"></select>
                <button id="playButton">Играть</button>
            </div>
        </div>
        <div id="frameSecond" class="app-layer frame-fix"></div>
    </div>
    <script src="./assets/js/libs/Animation.js"></script>
    <script src="./assets/js/scripts/renderer.js"></script>
</body>
</html>