{
  "name": "tjmc-launcher",
  "version": "1.0.0",
  "description": "An ElectronJS version if TJM-Launcher",
  "main": "index.js",
  "scripts": {
    "test": "electron .",
    "dist": "node build.js",
    "dist:win": "npm run dist -- WINDOWS",
    "dist:mac": "npm run dist -- MAC",
    "dist:linux": "npm run dist -- LINUX"
  },
  "keywords": [
    "Minecraft",
    "Launcher"
  ],
  "author": "MakAndJo",
  "license": "ISC",
  "dependencies": {
    "adm-zip": "^0.5.1",
    "checksum": "^0.1.1",
    "ejs": "^3.1.5",
    "ejs-electron": "^2.1.1",
    "jquery": "^3.5.1",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "electron": "^11.1.0",
    "electron-packager": "^15.2.0"
  },
  "build": {
    "appId": "com.trinityinfosystem.electron.exchange.stream",
    "productName": "Accurate",
    "copyright": "Copyright Â© 2018 Trinity InfoSystem",
    "win": {
      "publisherName": "Trinity InfoSystem",
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  }
}
